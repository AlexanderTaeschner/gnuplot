/*
 From the introduction of enhanced text support to gnuplot,
 there were native implementations for postscript and PM
 terminals.
 This has been replaced by a generic approach (a terminal
 API) in 2004. As there do exist applications which use
 gnupmdrv.exe directly we still keep the old implementation
 and binary interface.
*/
#define PM_KEEP_OLD_ENHANCED_TEXT 1

/*
 You can still use the old enhanced text code. Note that it does
 not support features like overprinting and Unicode escapes.
*/
/* #undef PM_OLD_ENHANCED_TEXT */

#if defined(PM_OLD_ENHANCED_TEXT) && !defined(PM_KEEP_OLD_ENHANCED_TEXT)
#define PM_KEEP_OLD_ENHANCED_TEXT 1
#endif

#define strtoll _strtoll

/* Some backwards compatibility with EMX */
#ifndef __KLIBC__
# define int32_t int
# define uint32_t unsigned int
# define isblank(x) isspace(x)
# define isinf(x) (fpclassify(x) == FP_INFINITE)
# define atoll _atoll
# define round(x) ((x >= 0.0) ? floor(x + 0.5) : ceil(x - 0.5))
# define lround(a) (long)(round(a))
# define llround(a) (long long)(round(a))
# define useconds_t unsigned
#endif
